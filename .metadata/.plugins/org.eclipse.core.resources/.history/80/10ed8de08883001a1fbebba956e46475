package sync;

public class User extends Thread{
	String name;
	Toilet toilet;
	public User(String name, Toilet toilet) {
		super();
		this.name = name;
		this.toilet = toilet;
	}
	public Toilet getToilet() {
		return toilet;
	}
	public void setToilet(Toilet toilet) {
		this.toilet = toilet;
	}
	
	
}
